      <Button fx:id="exitButton" layoutX="342.0" layoutY="264.0" mnemonicParsing="false" onMouseClicked="#onExitClick" prefHeight="36.0" prefWidth="131.0" text="退出" textFill="#141414" />
   </children>
</AnchorPane>
